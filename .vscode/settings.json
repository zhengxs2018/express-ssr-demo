{
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "search.followSymlinks": false,
  "search.exclude": {
    "**/.history/**": true,
    "**/run/": true,
    "**/public/uploads/**": true
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1500,
  "files.exclude": {
    ".history": true
  },
  "files.watcherExclude": {
    "**/.history/**": true,
    "**/run/": true,
    "**/public/uploads/**": true
  },
  "cSpell.words": [
    "Nylas",
    "ognomy",
    "staticfile"
  ],
  "cSpell.ignoreWords": [
  ],
  "cSpell.ignorePaths": [
    "**/.history/**",
    "**/public/uploads/**",
    "**/run/**",
  ]
}
